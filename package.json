    "@types/mz": "^0.0.32",
    "@types/node-fetch": "^2.5.3",
    "@types/shelljs": "^0.8.6",
    "bunyan": "^1.8.12",
    "chai": "4.2.0",
    "clarify": "2.1.0",
    "doctoc": "1.4.0",
    "jscpd": "^2.0.16",
    "mocha": "5.2.0",
    "mochawesome": "3.1.1",
    "node-fetch": "2.3.0",
