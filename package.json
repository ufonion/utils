    "tsdoc": "typedoc && touch docs/.nojekyll",
    "npm_publish": "scripty"
  },
  "devDependencies": {
    "@types/chai": "4.1.7",
    "@types/lodash": "^4.14.144",
    "@types/minimist": "^1.2.0",
